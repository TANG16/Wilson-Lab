<?xml version="1.0" encoding="utf-8"?>

<pumpProbeStandardsSet>
	<description>
	The inverted EU/PHEO standards set: (t,x) => (t,-x)
	Inverted for the purpose of use when the data set is inverted
	The original standards sets are from the spectroscopy cuvettes used in Tom's Sci. Transl. Med. paper
	</description>

	<pumpProbeStandard>
		<name>eumelanin</name>	
		<description>1 mg/mL eumelanin in cuvette</description>

		<pumpWavelength>720</pumpWavelength>
		<probeWavelength>810</probeWavelength>


		<color_red>1.0</color_red>
		<color_green>0.0</color_green>
		<color_blue>0.0</color_blue>

		<fileName>invertedEumelanin720pump810probe.mat</fileName>

		<scale>1.0</scale>
	</pumpProbeStandard>

	<pumpProbeStandard>
		<name>pheomelanin</name>	
		<description>1 mg/mL pheomelanin in cuvette</description>

		<pumpWavelength>720</pumpWavelength>
		<probeWavelength>810</probeWavelength>


		<color_red>0.0</color_red>
		<color_green>1.0</color_green>
		<color_blue>0.0</color_blue>

		<fileName>invertedPheomelanin720pump810probe.mat</fileName>

		<scale>1.0</scale>
	</pumpProbeStandard>

</pumpProbeStandardsSet>
